{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/types/(.*)": "<rootDir>/../libs/types/src/$1",
    "@app/types": "<rootDir>/../libs/types/src",
    "@app/helpers/(.*)": "<rootDir>/../libs/helpers/src/$1",
    "@app/helpers": "<rootDir>/../libs/helpers/src",
    "@app/core/(.*)": "<rootDir>/../libs/core/src/$1",
    "@app/core": "<rootDir>/../libs/core/src",
    "@app/models/(.*)": "<rootDir>/../libs/models/src/$1",
    "@app/models": "<rootDir>/../libs/models/src"
  }
}